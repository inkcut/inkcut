from enaml.widgets.api import IPythonConsole
from inkcut.core.api import DockItem
from inkcut.core.utils import load_icon


enamldef ConsoleDockItem(DockItem):
    title = 'Console'
    name = 'console-item'
    attr plugin
    icon = load_icon('application_xp_terminal')
